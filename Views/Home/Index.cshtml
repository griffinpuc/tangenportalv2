    <script>

        function MainLoad() {
            $.ajax({
                url: '@Url.Action("runMain", "Home")',
                success: function (data) {
                    $('#mainview').html(data);
                }
            });
        }

        function RunLoad(pagenum) {
            $.ajax({
                url: '@Url.Action("runMain", "Home")',
                data: { pagenum },
                success: function (data) {
                    $('#mainview').html(data);
                }
            });
        }

    </script>

